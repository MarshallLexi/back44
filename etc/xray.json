{
    "inbounds": [
        {
            "listen": "/etc/caddy/vless", 
            "protocol": "vless", 
            "settings": {
                "clients": [
                    {
                        "id": "$UUID"
                    }
                ], 
                "decryption": "none"
            }, 
            "streamSettings": {
                "network": "ws", 
                "wsSettings": {
                    "path": "/vless"
                }
            }
        },
		{
      "listen": "/etc/caddy/vmess",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "$UUID",
            "alterId": 64
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/vmess"
        }
      }
    }
    ], 
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ]
}
